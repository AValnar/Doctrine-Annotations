{
    "name": "av/doctrine-annotations",
    "description": "Simple Doctrine Annotations adapter for projects with silent auto loading",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
      "php": ">=5.6.0",
      "doctrine/annotations": "^1.2",
      "symfony/options-resolver": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "AValnar\\Doctrine\\": ["src"],
            "AValnar\\Tests\\Doctrine\\": ["tests"]
        }
    }
}